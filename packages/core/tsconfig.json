{
  "extends": "../../tsconfig.base.json",
  "include": [
    "./src"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "src/generator/_tests_",
    "src/**/*/_gen.ts",
    "generated"
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "target": "ESNext",
    "module": "CommonJS",
    "types": [
      "vitest/importMeta",
      "vitest/globals",
      "vite/client"
    ],
    "jsx": "preserve",
    "preserveConstEnums": true
  }
}